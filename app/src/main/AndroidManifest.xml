<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.fermedicius">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EnterName"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnterAge"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EnterGender"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EnterTraining"
            android:screenOrientation="portrait" />
        <activity android:name=".EnterExercise" />
        <activity
            android:name=".EnterSports"
            android:screenOrientation="portrait" />
        <activity android:name=".Final"></activity>
    </application>

</manifest>